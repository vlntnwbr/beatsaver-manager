    "version": "0.2.0",
    "configurations": [
        {  // err: --log-level
            "name": "[ ERR ] --log-level",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": ["--log-level", "NOT_A_VALID_LOG_LEVEL"],
            "env": {"BEATSABER": "./.beatsaber"}
        },
        {  // err: --beatsaber
            "name": "[ ERR ] --beatsaber",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": ["--beatsaber", "NONEXISTENT_DIRECTORY"],
            "env": {"BSDL_LOG_LEVEL": "debug"}
        },
        {  // bpl install
            "name": "bpl install",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": [
                "bpl", "install",
                "https://beatsaver.com/playlists/3445",
                "https://vweber.eu",
                "https://api.beatsaver.com/playlists/id/3351/download"
            ],
            "env": {
                "BEATSABER": "./.beatsaber",
                "BSDL_LOG_LEVEL": "debug"
            }
        },
        {  // bpl install --keys
            "name": "bpl install --keys",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": [
                "bpl", "install", "--keys", "3445", "abcd", "3444"
            ],
            "env": {
                "BEATSABER": "./.beatsaber",
                "BSDL_LOG_LEVEL": "debug"
            }
        },
        {  // bpl install --force --files
            "name": "bpl install --files",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": [
                "bpl", "install", "--force", "--files",
                ".beatsaber/.test_data/_err.bplist",
                ".beatsaber/.test_data/3351.bplist",
            ],
            "env": {
                "BEATSABER": "./.beatsaber",
                "BSDL_LOG_LEVEL": "debug"
            }
        },
        {  // bpl list
            "name": "bpl list",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": ["bpl", "list"],
            "env": {
                "BEATSABER": "./.beatsaber",
                "BSDL_LOG_LEVEL": "debug"
            }
        },
        {  // bpl list --outdated
