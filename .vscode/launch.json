        {  // bpl rm
            "name": "bpl rm",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
            "args": ["bpl", "rm", "2218", "3351"],
            "env": {
                "BEATSABER": "./.beatsaber",
                "BSDL_LOG_LEVEL": "debug"
            }
        },
        {  // bpl rm --files
            "name": "bpl rm --files",
            "type": "python",
            "request": "launch",
            "module": "bsdl.cli.main",
